{
  "name": "easy-notes",
  "version": "0.1.1",
  "description": "",
  "main": "main.js",
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --mode development",
    "serve": "run-electron .",
    "start-e": "cross-env START_HOT=1 yarn start",
    "build-e": "cross-env NODE_ENV=production webpack --progress --colors --profile -p",
    "win": "cross-env NODE_ENV=production build --win --x64",
    "linux": "build --linux",
    "mac": "cross-env NODE_ENV=production build --mac",
    "test": "jest",
    "test:watch": "npm run test --watch",
    "test:cover": "npm run test --coverage",
    "pack": "electron-builder --dir",
    "build:macos": "yarn build-e && cross-env NODE_ENV=production electron-builder --mac --publish=never",
    "dist": "electron-builder",
    "postinstall": "install-app-deps"
  },
  "build": {
    "productName": "Easy-Notes",
    "appId": "omatic.app.easy.note",
    "mac": {
      "category": "your.app.category.type"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "asar": false,
    "files": [
      "build_code/**/*",
      "main.js",
      "package.json"
    ],
    "win": {
      "asar": false,
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "maintainer": "Andrey Savchuk <andrej.savchuk@gmail.com>",
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "public",
      "output": "build"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "sideEffects": false,
